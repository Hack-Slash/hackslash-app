<body data-spy="scroll" data-target="#main-navbar">
    <div class="page-loader"></div>  <!-- Display loading image while page loads -->
    <div class="body">  
        <section id="portfolio-section" class="page bg-style1">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="portfolio">
                            <div class="portfoloi_content_area" >
                                <div class="portfolio_content">
                                    <div class="row"  id="portfolio">
                                        <% @people.each do |person| %>
                                            <div class="col-sm-4">
                                                <div class="portfolio_single_content">
                                                    <img src="/images/117840_big.jpg" alt="title"/>
                                                    <div>
                                                        <!-- NAME WILL BE REMOVED -->
                                                        <a><%= person[:name] %></a>
                                                        <span class="test"><%= person[:note] %></span>
                                                    </div>
                                                </div>
                                            </div>
                                        <% end %>
                                        <% 12.times do %>
                                            <div class="col-sm-4">
                                                <div class="portfolio_single_content">
                                                    <img src="/images/117840_big.jpg" alt="title"/>
                                                </div>
                                            </div>
                                        <% end %>
                                        <div class="col-sm-4">
                                            <div class="portfolio_single_content">
                                                <img src="/images/117840_yknow.jpg" alt="title"/>
                                                <div>
                                                    <a href="/yknow">We didn't forget <br> about you Kenny <br> <br> You know?</a>
                                                </div>
                                            </div>
                                        </div>
                                        <% 13.times do %>
                                            <div class="col-sm-4">
                                                <div class="portfolio_single_content">
                                                    <img src="/images/117840_big.jpg" alt="title"/>
                                                </div>
                                            </div>
                                        <% end %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</body>